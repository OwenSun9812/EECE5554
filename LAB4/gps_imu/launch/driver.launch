<launch>
    <arg name="gps_port" default="/dev/ttyUSB1"/>
    <arg name="imu_port" default="/dev/ttyUSB0"/>

    <node pkg="gps_imu" type="service_server.py" name="convert_imu_service_server" output="screen">
    </node>
    <node pkg="gps_imu" type="imu_driver.py" name="imu_Publisher" output="screen">
    <param name="imu_port" value="$(arg imu_port)"/>    
    </node> 
    
    <node pkg = "gps_imu" type = "gps_driver.py" name = "gps_Publisher" output = "screen">
    <param name="gps_port" value="$(arg gps_port)"/>    
    </node> 

</launch>
